{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "sites_edit": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::site.site",
      "inversedBy": "editors"
    },
    "firstName": {
      "type": "string"
    },
    "sites_own": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::site.site",
      "mappedBy": "owner"
    },
    "artists_own": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::artist.artist",
      "mappedBy": "owner"
    },
    "artists_edit": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::artist.artist",
      "mappedBy": "editors"
    },
    "themes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::theme.theme",
      "mappedBy": "owner"
    }
  }
}
