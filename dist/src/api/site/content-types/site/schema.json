{
    "kind": "collectionType",
    "collectionName": "sites",
    "info": {
        "singularName": "site",
        "pluralName": "sites",
        "displayName": "Site",
        "description": ""
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "name": {
            "type": "string"
        },
        "customDomain": {
            "type": "string",
            "unique": false
        },
        "editors": {
            "type": "relation",
            "relation": "manyToMany",
            "target": "plugin::users-permissions.user",
            "mappedBy": "sites_edit"
        },
        "owner": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "plugin::users-permissions.user",
            "inversedBy": "sites_own"
        },
        "subdomain": {
            "type": "uid",
            "targetField": "name",
            "required": true
        },
        "about": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::about.about",
            "mappedBy": "site"
        },
        "type": {
            "type": "enumeration",
            "enum": [
                "artist",
                "label"
            ]
        },
        "artist": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::artist.artist",
            "mappedBy": "site"
        },
        "theme": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::theme.theme",
            "inversedBy": "site"
        },
        "homePage": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::home-page.home-page",
            "inversedBy": "site"
        }
    }
}
